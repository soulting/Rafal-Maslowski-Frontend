{"ast":null,"code":"import { ref } from \"vue\";\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const menuOpen = ref(false);\n    const openMenu = () => {\n      menuOpen.value = !menuOpen.value;\n    };\n    const __returned__ = {\n      menuOpen,\n      openMenu,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","menuOpen","openMenu","value"],"sources":["/home/andrzej/01_Programing/rafal_maslowski_front/rafal_maslowski_front/src/App.vue"],"sourcesContent":["<template>\n  <img class=\"navbar-img-shadow\" src=\"@/assets/RM_logo.png\" alt=\"icon shadow\" />\n  <nav>\n    <router-link to=\"/\">\n      <img class=\"navbar-img\" src=\"@/assets/RM_logo.png\" alt=\"icon image\" />\n    </router-link>\n    <div class=\"tab-container\">\n      <router-link class=\"router-link\" to=\"/about\">BLOG </router-link>\n      <router-link class=\"router-link\" to=\"/about\">O MNIE </router-link>\n      <router-link class=\"router-link\" to=\"/about\">KONTAKT </router-link>\n      <router-link class=\"router-link\" to=\"/about\">KREDYTY </router-link>\n      <router-link class=\"router-link\" to=\"/about\">PRZYDATNE </router-link>\n    </div>\n    <button\n      class=\"tab-hamburger-menu\"\n      @click=\"openMenu()\"\n      :class=\"{ 'open-menu': menuOpen }\"\n    >\n      <span class=\"line-1\"></span>\n      <span class=\"line-2\"></span>\n      <span class=\"line-3\"></span>\n    </button>\n  </nav>\n  <transition name=\"dropdownmenuExpansion\">\n    <div v-if=\"menuOpen\" class=\"dropdown-tabs\">\n      <router-link class=\"router-link\" to=\"/about\">BLOG </router-link>\n      <router-link class=\"router-link\" to=\"/about\">O MNIE </router-link>\n      <router-link class=\"router-link\" to=\"/about\">KONTAKT </router-link>\n      <router-link class=\"router-link\" to=\"/about\">KREDYTY </router-link>\n      <router-link class=\"router-link\" to=\"/about\">PRZYDATNE </router-link>\n    </div>\n  </transition>\n\n  <router-view class=\"c\" />\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\n\nconst menuOpen = ref(false);\n\nconst openMenu = () => {\n  menuOpen.value = !menuOpen.value;\n};\n</script>\n\n<style>\nbody {\n  margin: 0;\n}\n\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  overflow: hidden;\n}\n\nnav {\n  height: 70px;\n  display: flex;\n  align-items: flex-start;\n  justify-content: space-between;\n  padding: 5px 10px 5px 10px;\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);\n  background-color: white;\n  position: relative;\n  top: 0;\n  left: 0;\n  right: 0;\n}\n\n.navbar-img {\n  height: 100px;\n}\n\n.navbar-img-shadow {\n  height: 100px;\n  position: absolute;\n  top: 0;\n  left: 10px;\n  filter: drop-shadow(0px 3px 4px rgb(156, 156, 156));\n}\n\n.tab-container {\n  display: none;\n  align-items: center;\n  height: 70px;\n  gap: 20px;\n}\n\n.tab-hamburger-menu {\n  height: 50px;\n  width: 50px;\n  margin-top: 10px;\n  background-color: rgb(255, 255, 255);\n  border: none;\n  position: relative;\n}\n\n.tab-hamburger-menu span {\n  background-color: rgb(0, 0, 0);\n  position: absolute;\n  width: 100%;\n  height: 6px;\n  border-radius: 3px;\n  top: 50%;\n  left: 50%;\n  transition: all 0.5s;\n}\n\n.line-1 {\n  transform: translate(-50%, -20px);\n}\n.line-2 {\n  transform: translate(-50%, -50%);\n}\n.line-3 {\n  transform: translate(-50%, 15px);\n}\n\n.open-menu .line-1 {\n  transform: translate(-50%, -50%) rotate(-45deg);\n}\n\n.open-menu .line-2 {\n  width: 0;\n}\n\n.open-menu .line-3 {\n  transform: translate(-50%, -50%) rotate(45deg);\n}\n\n.router-link {\n  text-decoration: none;\n  color: black;\n  font-size: 15px;\n  font-weight: 500;\n  height: 30px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  transition: all 0.3s ease;\n}\n\n.router-link:hover {\n  color: rgb(67, 149, 209);\n  font-size: 17px;\n  padding: 0 10px;\n}\n\n.dropdown-tabs {\n  display: flex;\n  top: 80px;\n  left: 0;\n  right: 0;\n  padding: 0 15px 0 15px;\n  background-color: white;\n  flex-direction: column;\n  align-items: flex-end;\n  position: absolute;\n  z-index: -1;\n  /* box-shadow: 0px 10px 15px -8px rgb(81, 81, 81); */\n}\n\n.dropdown-tabs .router-link {\n  border-top: solid rgb(100, 100, 100) 0.5px;\n  width: 100%;\n  height: 50px;\n  display: flex;\n  align-items: center;\n  justify-content: end;\n  padding-right: 35px;\n  box-sizing: border-box;\n}\n\n.dropdownmenuExpansion-enter-from,\n.dropdownmenuExpansion-leave-to {\n  opacity: 0;\n  transform: translateY(-100%);\n}\n\n.dropdownmenuExpansion-enter-active,\n.dropdownmenuExpansion-leave-active {\n  transition: all 0.5s ease;\n}\n\n.dropdownmenuExpansion-enter-to,\n.dropdownmenuExpansion-leave-from {\n  opacity: 1;\n  transform: translateY(0%);\n}\n\n@media (min-width: 450px) {\n  nav {\n    padding: 5px 60px 5px 60px;\n  }\n\n  .navbar-img-shadow {\n    left: 60px;\n  }\n  .dropdown-tabs {\n    left: auto;\n    right: 50px;\n    width: 350px;\n    overflow: hidden;\n  }\n  .dropdown-tabs .router-link {\n    padding-right: 65px;\n  }\n}\n\n@media (min-width: 768px) {\n  .tab-container {\n    display: flex;\n  }\n\n  .dropdown-tabs,\n  .tab-hamburger-menu {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AAqCA,SAASA,GAAG,QAAQ,KAAK;;;;;;;IAEzB,MAAMC,QAAQ,GAAGD,GAAG,CAAC,KAAK,CAAC;IAE3B,MAAME,QAAQ,GAAGA,CAAA,KAAM;MACrBD,QAAQ,CAACE,KAAK,GAAG,CAACF,QAAQ,CAACE,KAAK;IAClC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}