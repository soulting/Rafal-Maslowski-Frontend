sts from \"@/views/BlogViewElements/Posts.vue\";\nimport { ref } from \"vue\";\n\nconst category = ref(\"WSZYSTKIE WPISY\");\n\nconst showSection = ref(false);\n\nconst changeCategory = (item) => {\n  category.value = item;\n};\n</script>\n\n<style scoped>\n.loading-image {\n  width: 150px; /* Dostosuj rozmiar logo */\n}\n\n.dot-container {\n  display: flex;\n  gap: 30px;\n}\n\n.dot {\n  height: 20px;\n  width: auto;\n}\n\n#first {\n  animation: dot-animation 1.5s infinite;\n  animation-delay: 0.25s;\n}\n\n#second {\n  animation: dot-animation 1.5s infinite;\n  animation-delay: 0.5s;\n}\n\n#third {\n  animation: dot-animation 1.5s infinite;\n  animation-delay: 0.75s;\n}\n\n@keyframes dot-animation {\n  0% {\n    scale: 1;\n  }\n  50% {\n    scale: 1.7;\n  }\n  100% {\n    scale: 1;\n  }\n}\n.post-view-post-container-loader {\n  height: 100vh;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  gap: 30px;\n  position: absolute;\n  z-index: 10;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  background-color: white;\n}\n</style>\n"],"mappings":"AAiBA,OAAOA,OAAO,MAAM,sCAAsC;AAC1D,OAAOC,KAAK,MAAM,oCAAoC;AACtD,OAAOC,SAAS,MAAM,2CAA2C;AACjE,OAAOC,KAAK,MAAM,oCAAoC;AACtD,SAASC,GAAG,QAAQ,KAAK;;;;;;;IAEzB,MAAMC,QAAQ,GAAGD,GAAG,CAAC,iBAAiB,CAAC;IAEvC,MAAME,WAAW,GAAGF,GAAG,CAAC,KAAK,CAAC;IAE9B,MAAMG,cAAc,GAAIC,IAAI,IAAK;MAC/BH,QAAQ,CAACI,KAAK,GAAGD,IAAI;IACvB,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}