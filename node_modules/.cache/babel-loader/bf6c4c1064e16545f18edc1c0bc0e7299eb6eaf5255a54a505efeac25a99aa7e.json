{"ast":null,"code":"import Footer from \"./SharedElements/Footer.vue\";\nimport Contact from \"@/views/HomeViewElements/Contact.vue\";\nimport { onMounted, ref } from \"vue\";\nexport default {\n  __name: 'AboutView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const showElement = ref(false);\n    onMounted(() => {\n      window.scroll(0, 0);\n      observer = new IntersectionObserver(entries => {\n        entries.forEach(entry => {\n          if (entry.isIntersecting) {\n            showElement.value = true;\n          }\n        });\n      });\n      const target = document.getElementById(\"about-element-observe-target\");\n      if (target) observer.observe(target);\n    });\n    const __returned__ = {\n      showElement,\n      Footer,\n      Contact,\n      onMounted,\n      ref\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["Footer","Contact","onMounted","ref","showElement","window","scroll","observer","IntersectionObserver","entries","forEach","entry","isIntersecting","value","target","document","getElementById","observe"],"sources":["/home/andrzej/01_Programing/rafal_maslowski_front/src/views/AboutView.vue"],"sourcesContent":["<template>\n  <section class=\"about\" id=\"about-element-observe-target\">\n    <div class=\"upper-image-container\">\n      <img\n        src=\"@/assets/backgrounds/kevin-rajaram-ULwzqOnPem0-unsplash.jpg\"\n        alt=\"background-image\"\n      />\n      <div class=\"name-container\" id=\"element0\"><h1>Rafał Masłowski</h1></div>\n    </div>\n    <div class=\"about-main-section\">\n      <div class=\"description-section\" id=\"element1\">\n        <h2>Doradca kredytowy</h2>\n        <p>\n          Wiedzę i doświadczenie zdobyłem od podszewki, pracując w banku. W\n          życiu zawodowym kieruję się przede wszystkim dobrem klienta. Stawiam\n          na indywidualne podejście, co w moim przekonaniu jest kluczem do\n          skutecznego prowadzenia całego procesu kredytowego. Dzielę się swoją\n          wiedzą, staram się też przybliżyć dobre praktyki eksperta kredytowego.\n        </p>\n        <button>Skontaktuj się</button>\n      </div>\n      <img\n        id=\"element2\"\n        src=\"@/assets/backgrounds/428523336_7659326927521439_9053629739761281834_n.jpg\"\n        alt=\"profile picture\"\n      />\n    </div>\n  </section>\n  <Contact /><Footer />\n</template>\n\n<script setup>\nimport Footer from \"./SharedElements/Footer.vue\";\nimport Contact from \"@/views/HomeViewElements/Contact.vue\";\nimport { onMounted, ref } from \"vue\";\n\nconst showElement = ref(false);\nonMounted(() => {\n  window.scroll(0, 0);\n\n  observer = new IntersectionObserver((entries) => {\n    entries.forEach((entry) => {\n      if (entry.isIntersecting) {\n        showElement.value = true;\n      }\n    });\n  });\n\n  const target = document.getElementById(\"about-element-observe-target\");\n  if (target) observer.observe(target);\n});\n</script>\n\n<style scoped>\n.about {\n  margin-top: 70px;\n}\n\n.upper-image-container {\n  align-items: flex-end;\n  color: white;\n  display: flex;\n  height: 200px;\n  justify-content: center;\n  position: relative;\n}\n\n.upper-image-container img {\n  left: 0;\n  position: absolute;\n  right: 0;\n  width: 450px;\n  z-index: -1;\n}\n\n.about-main-section {\n  align-items: center;\n  background-color: white;\n  display: flex;\n  font-style: italic;\n  justify-content: center;\n  line-height: 25px;\n  padding: 20px;\n}\n\n.about-main-section img {\n  border-radius: 5px;\n  display: none;\n  width: 100%;\n}\n\n.name-container {\n  align-items: center;\n  background-color: rgba(19, 19, 19, 0.6);\n  bottom: 20px;\n  display: flex;\n  flex-direction: column;\n  height: 100px;\n  justify-content: center;\n  left: 50%;\n  position: absolute;\n  transform: translateX(-50%);\n  width: 300px;\n  z-index: 5;\n}\n\n.name-container h1 {\n  color: white;\n  font-size: 30px;\n  margin: 0;\n}\n\n.description-section {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  text-align: justify;\n}\n\n.description-section button {\n  background-color: white;\n  border: solid black 0.5px;\n  border-radius: 25px;\n  color: #2c3e50;\n  cursor: pointer;\n  font-family: \"Quicksand\", sans-serif;\n  font-optical-sizing: auto;\n  font-size: 16px;\n  font-weight: 500;\n  height: 50px;\n  margin-bottom: 30px;\n  margin-top: 30px;\n  padding: 0 15px;\n  text-align: center;\n  transition: all 1s ease;\n}\n\n.description-section button:hover {\n  background-color: #2c3e50;\n  color: #ffffff;\n  font-size: 18px;\n}\n\n#element0.aboutElement-enter-active {\n  transition-delay: 0.25s;\n}\n#element1.aboutElement-enter-active {\n  transition-delay: 0.75s;\n}\n\n#element2.aboutElement-enter-active {\n  transition-delay: 1.25s;\n}\n\n.aboutElement-enter-from,\n.aboutElement-leave-to {\n  opacity: 0;\n  transform: translateY(100%);\n}\n\n.aboutElement-enter-active,\n.aboutElement-leave-active {\n  transition: all 1s ease;\n}\n\n/* Media Queries */\n@media (min-width: 450px) {\n  .upper-image-container {\n    height: 250px;\n  }\n\n  .upper-image-container img {\n    bottom: -75px;\n    width: 650px;\n  }\n\n  .about-main-section {\n    padding: 30px;\n  }\n}\n\n@media (min-width: 650px) {\n  .upper-image-container {\n    height: 300px;\n  }\n\n  .upper-image-container img {\n    bottom: -100px;\n    width: 768px;\n  }\n\n  .about-main-section {\n    padding: 40px;\n  }\n}\n\n@media (min-width: 768px) {\n  .about-main-section {\n    gap: 30px;\n    padding-bottom: 75px;\n    padding-top: 75px;\n    font-size: 18px;\n    line-height: 30px;\n  }\n\n  .about-main-section img {\n    display: block;\n    flex: 1;\n    max-width: 450px;\n  }\n\n  .description-section {\n    flex: 1;\n    max-width: 450px;\n  }\n\n  .name-container {\n    height: 100px;\n    width